# Full path to the System Under Test (or Java Main Class name)
interpreter:java
executable:org.yatzy.cli.CliAdapterYatzy1

# Naming scheme to use for files for stdin,stdout and stderr
filename_convention_scheme:standard

# Expanded name to use for application
full_name:Yatzy

default_checkout:yatzy

extra_search_directory:${TEXTTEST_CHECKOUT}/target/texttest_extra_config/

# Do replace these with your favourite tools
diff_program:meld
view_program:subl
